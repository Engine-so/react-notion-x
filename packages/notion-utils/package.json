{
  "name": "en-notion-utils",
  "version": "6.12.13",
  "description": "Useful utilities for working with Notion data. Isomorphic.",
  "repository": "Engine-so/react-notion-x",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --silent --onSuccess 'echo build successful'",
    "test": "ava"
  },
  "dependencies": {
    "is-url-superb": "^6.1.0",
    "mem": "^9.0.2",
    "normalize-url": "^7.0.3",
    "notion-types": "npm:en-notion-types@^6.12.9",
    "p-queue": "^7.2.0"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ]
  },
  "gitHead": "b693f60b85e8053636d85855888a4c88e5ef08a7"
}
